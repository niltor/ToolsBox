<Window x:Class="DataHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataHelper"
        mc:Ignorable="d"
        Title="MainWindow" Height="470" Width="399">
    <StackPanel>
        <StackPanel Height="37" Orientation="Horizontal">
            <TextBox x:Name="FilePath" TextWrapping="Wrap" Width="304" Height="22"/>
            <Button x:Name="button" Content="选择文件" Height="24" Width="90" Click="button_Click"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="button1" Content="导入用户" Height="25" Width="100" Click="button1_ClickAsync" Margin="8"/>
            <Button x:Name="button2" Content="导入人员" Height="25" Width="100" Margin="8" Click="button2_ClickAsync"/>
            <Button x:Name="button3" Content="导入组织信息" Height="25" Width="100" Margin="8" Click="button3_ClickAsync"/>
        </StackPanel>

        <TextBox x:Name="OutputMsg" TextWrapping="Wrap" Width="382" Height="361"/>

    </StackPanel>

</Window>
